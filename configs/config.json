{
  "server": {
    "port": 8080
  },
  "balancer_type": "round-robin",
  "backends": [
    {
      "url": "http://localhost:8081",
      "weight": 1,
      "max_connections": 100
    },
    {
      "url": "http://localhost:8082",
      "weight": 1,
      "max_connections": 100
    },
    {
      "url": "http://localhost:8083",
      "weight": 1,
      "max_connections": 100
    }
  ],
  "rate_limit": {
    "enabled": true,
    "default_rate": 10,
    "default_capacity": 100
  },
  "health_check": {
    "interval": 10,
    "timeout": 2
  }
}